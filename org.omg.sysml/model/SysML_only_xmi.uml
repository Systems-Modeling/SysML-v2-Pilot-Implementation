            <language>OCL2.0</language>
            <body>specializesFromLibrary('Actions::Action::join')</body>
          </specification>
        </ownedRule>
        <generalization xmi:id="_18_5_3_12e503d9_1565503206599_147052_33705" general="_19_0_2_12e503d9_1567281323333_776611_544"/>
