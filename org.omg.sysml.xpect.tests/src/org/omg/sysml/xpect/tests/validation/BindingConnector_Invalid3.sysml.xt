package Test {
	
	block A;
	block B;
	block C specializes B;
	
	part P {
		part a: A {
			in x: ScalarValues::Integer;
		}
		
		part b: B {
			out x;
			out y: ScalarValues::String;
		}
		
		part c: C {
			in y;
		}
		
		// XPECT errors --> "Binding connector ends must have conforming types" at "a = b;"
		bind a = b;
		// XPECT errors --> "Output feature must conform to input feature" at "a.x = b.x;"
		bind a.x = b.x;
		
		bind b = c;
		bind b.y = c.y;
	}
}

