package 'ValueProperty Example' {
	port def P;
	block A {
		part a: A;
		port p: P;
	}
	block Vehicle {
		// XPECT errors --> "A value property must be typed by value types." at "a : A;"
		value a : A;
		// XPECT errors --> "A value property must be typed by value types." at "b : A::a;"
		value b : A::a;
		// XPECT errors --> "A value property must be typed by value types." at "s : P;"
		value s : P;
		//XPECT errors --> "A value property must be typed by value types." at "un : A::p;"
		value un: A::p;
		//XPECT errors --> "A value property must be typed by value types." at "n : AB;"
		value n: AB;
		}
	assoc block  AB {
		end : A;
		end : A;
	}
}