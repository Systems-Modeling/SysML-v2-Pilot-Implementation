package 'Port Example' {
	value type Vt;
	block B;
	port def pd1;
	port def pd2;
	block FA {
		/* XPECT errors --- 
		   	"A port must be typed by one port definition." at "ablock : B;"
			"Features must have at least one type" at "ablock : B;"
		--- */
		port ablock : B;
		/* XPECT errors --- 
		   	"A port must be typed by one port definition." at "avaluetype : Vt;"
			"Features must have at least one type" at "avaluetype : Vt;"
		--- */
		port avaluetype : Vt;
		//XPECT errors --> "A port must be typed by one port definition." at "two_port_def_types : pd1, pd2;"
		port two_port_def_types: pd1, pd2;
	}
	part tank: FA {
		/* XPECT errors --- 
		   "A port must be typed by one port definition." at "aport redefines ablock;"
		   "Features must have at least one type" at "aport redefines ablock;"
		 --- */
		port aport redefines ablock;
	}
}