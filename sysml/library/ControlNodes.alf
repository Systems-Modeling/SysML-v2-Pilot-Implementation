package ControlNodes {
	private import Base::*;
	
	behavior Merge() {
		feature predecessors: Occurrence[1] redefines Performance::predecessors;
	}
	
	behavior Decision() {
		feature successors: Occurrence[1] redefines Performance::successors;
	}
	
	behavior Join() {
		feature predecessors: Occurrence[2] redefines Performance::predecessors;
	}
	
	behavior Fork() {
		feature successors: Occurrence[2] redefines Performance::successors;
	}
	
 }